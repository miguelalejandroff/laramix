{
    "name": "miguelalejandroff/laramix",
    "description": "Informix Driver for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "ifx",
        "informix",
        "laravel",
        "laravel 6",
        "laravel 7",
        "laravel-framework"
    ],
    "authors": [
        {
            "name": "Arief BP",
            "email": "ariefbp27@gmail.com",
            "homepage": "https://github.com/miguelalejandroff/laramix"
        }
    ],
    "require": {
        "ext-pdo": "*",
        "php": "^7.4|^8.0",
        "guzzlehttp/guzzle": "^7.4|^8.0",
        "illuminate/support": "^7.4|^9.0",
        "illuminate/database": "^7.4|^8.0",
        "illuminate/pagination": "^7.4|^8.0",
        "illuminate/encryption": "^7.4|^8.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.10",
        "phpunit/phpunit": "~4.0",
        "phpspec/phpspec": "~2.1",
        "mockery/mockery": "^1.3.1"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Miguelalejandroff\\Laravel\\Ifx\\": "src/Ifx/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Miguelalejandroff\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Miguelalejandroff\\Laravel\\Ifx\\InformixServiceProvider"
            ]
        }
    }
}
